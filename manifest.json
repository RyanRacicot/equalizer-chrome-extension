{
    "manifest_version": 2,
    "name": "Equalizer",
    "version": "1.0",
    "description": "Google Chrome Tab Equalizer and General Audio Manager",
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "tabCapture",
        "storage"
    ],
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": "icons/get_started16.png",
        "default_popup": "popup.html"
    }
}