{
    "manifest_version": 2,
    "name": "Tab Equalizer",
    "version": "1.0",
    "description": "Google Chrome Tab Equalizer",
    "permissions": [
        "tabs",
        "activeTab",
        "tabCapture"
    ],
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": "assets/mixer.png",
        "default_popup": "popup.html"
    }
}